    "/api/contacts": {
      "get": {
        "description": "Search and filter contacts with pagination support",
        "operationId": "list-contacts",
        "parameters": [
          {
            "description": "Filter by name",
            "explode": false,
            "in": "query",
            "name": "name",
            "schema": {
              "description": "Filter by name",
              "type": "string"
            }
          },
          {
            "description": "Filter by email",
            "explode": false,
            "in": "query",
            "name": "email",
            "schema": {
              "description": "Filter by email",
              "type": "string"
            }
          },
          {
            "description": "Filter by phone",
            "explode": false,
            "in": "query",
            "name": "phone",
            "schema": {
              "description": "Filter by phone",
              "type": "string"
            }
          },
          {
            "description": "Page number",
            "explode": false,
            "in": "query",
            "name": "page",
            "schema": {
              "default": 1,
              "description": "Page number",
              "format": "int64",
              "type": "integer"
            }
          },
          {
            "description": "Page size",
            "explode": false,
            "in": "query",
            "name": "size",
            "schema": {
              "default": 10,
              "description": "Page size",
              "format": "int64",
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WebResponseListContactResponse"
                }
              }
            },
            "description": "OK"
          },
          "default": {
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            },
            "description": "Error"
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "List contacts with search",
        "tags": [
          "Contacts"
        ]
      },
